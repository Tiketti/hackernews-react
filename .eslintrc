{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": "off",
    "react/prop-types": "off",
    "react/no-did-mount-set-state": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": ["error", {"allowAfterThis": true }]
  }
}